@article{schuemie_interpreting_2014,
	title = {Interpreting observational studies: {Why} empirical calibration is needed to correct p-values},
	volume = {33},
	issn = {02776715},
	doi = {10.1002/sim.5925},
	abstract = {Often the literature makes assertions of medical product effects on the basis of ' p {\textbackslash}textless 0.05'. The underlying premise is that at this threshold, there is only a 5\% probability that the observed effect would be seen by chance when in reality there is no effect. In observational studies, much more than in randomized trials, bias and confounding may undermine this premise. To test this premise, we selected three exemplar drug safety studies from literature, representing a case-control, a cohort, and a self-controlled case series design. We attempted to replicate these studies as best we could for the drugs studied in the original articles. Next, we applied the same three designs to sets of negative controls: drugs that are not believed to cause the outcome of interest. We observed how often p {\textbackslash}textless 0.05 when the null hypothesis is true, and we fitted distributions to the effect estimates. Using these distributions, we compute calibrated p-values that reflect the probability of observing the effect estimate under the null hypothesis, taking both random and systematic error into account. An automated analysis of scientific literature was performed to evaluate the potential impact of such a calibration. Our experiment provides evidence that the majority of observational studies would declare statistical significance when no effect is present. Empirical calibration was found to reduce spurious results to the desired 5\% level. Applying these adjustments to literature suggests that at least 54\% of findings with p {\textbackslash}textless 0.05 are not actually statistically significant and should be reevaluated.},
	number = {2},
	journal = {Statistics in Medicine},
	author = {Schuemie, Martijn J. and Ryan, Patrick B. and Dumouchel, William and Suchard, Marc A. and Madigan, David},
	year = {2014},
	pmid = {23900808},
	keywords = {Calibration, Hypothesis testing, Negative controls, Observational studies},
	pages = {209--218},
	file = {Attachment:/Users/suesong/Zotero/storage/CW7TLE44/Schuemie et al. - 2014 - Interpreting observational studies Why empirical calibration is needed to correct p-values.pdf:application/pdf}
}

