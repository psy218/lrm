	url = {http://proxy.lib.utk.edu:90/login?url=http://search.proquest.com/docview/1170965681?accountid=14766%5Cnhttp://utk-almaprimo.hosted.exlibrisgroup.com/openurl/01UTK/01UTK_SERVICES?url_ver=Z39.88-2004&rft_val_fmt=info:ofi/fmt:kev:mtx:dissertation&genre=disserta},
	abstract = {This dissertation is about prejudice. In particular, it examines the theoretical and ethical questions raised by research on implicit social biases. Social biases are termed "implicit" when they are not reported, though they lie just beneath the surface of consciousness. Such biases are easy to adopt but very difficult to introspect and control. Despite this difficulty, I argue that we are personally responsible for our biases and obligated to overcome them if they can bring harm to ourselves or to others. My dissertation addresses the terms of their removal. It is grounded in a comprehensive examination of empirical research and, as such, is a contribution to social psychology. Although implicit social biases significantly influence our judgment and action, they are not reducible to beliefs or desires. Rather, they constitute a class of their own. Understanding their particular character is vital to determining how to replace them with more preferable habits of mind. I argue for a model of interpersonal fluency, a kind of ethical expertise that requires transforming our underlying dispositions of thought, feeling, and action.},
	journal = {ProQuest Dissertations and Theses},
	author = {Madva, Alex},
	year = {2012},
	keywords = {Prejudice, Automaticity, 0394:Ethics, 0422:Philosophy, 0451:Social psychology, Alief, Ethics, Implicit bias, Mind, Philosophy, Psychology, religion and theology, Social psychology},
	pages = {269}
}

