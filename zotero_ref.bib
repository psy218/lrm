@article{blake_virtual_2008,
	title = {A virtual love triangle: {Mainstream} online dating sites, the companies that own them and women subscribers.},
	volume = {68},
	url = {http://ezproxy.library.uvic.ca/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2008-99031-037&site=ehost-live&scope=site},
	abstract = {Since its inception in the mid-1990s, online dating rapidly has increased in popularity and the revenues of online dating companies have increased along with it. The purpose of this dissertation research was to study the relationships of power between mainstream online dating sites (specifically Match.com, JDate.com and eHarmony.com) and women users. Guided by a theoretical framework consisting of political economy, feminism, ideology and hegemony to consider how power is manifest both materially and ideologically, the dissertation begins with contextualization of the current online dating environment, history of the Internet, social constructions and feminist critiques of marriage and romantic love, and the history of love and dating. To study online dating companies and ideology on online sites in relation to views of women users, data were gathered from three sources: online dating companies' documentation and secondary information about them; text on online dating companies' web sites; and a survey of women users. Of particular interest were ideologies about marriage and dating and gender in the construction of power. Findings provide insight into the power relationships between online dating companies and women users, pointing to an unequal power distribution between company power and individual control. Additionally, the structure of some online dating sites seems to reflect increasing corporate conglomeration, both online and offline, and decreasing user knowledge of corporate structures and policies. Regarding ideologies about marriage and dating and gender in the construction of power, findings show that the online dating sites disseminate traditional ideas about gender roles and tend to encourage women to seek "the one." Survey data from women users reveal different ideas about the importance of marriage in a woman's life, but women generally adhered to traditional ideas about men's roles in initiating offline communication and in serving as chivalrous suitors. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
	number = {8-A},
	journal = {Dissertation Abstracts International Section A: Humanities and Social Sciences},
	author = {Blake, Stephanie Helen},
	year = {2008},
	keywords = {Love, Internet, online dating sites, Business Organizations, Clients, companies, feminism, Feminism, Human Females, Political Economic Systems, political economy, Social Dating, virtual love triangle, women subscribers},
	pages = {3195},
	file = {Attachment:/Users/suesong/Zotero/storage/7UIDGJD2/2007 Mainstream online dating sites, the companies that own them and women subscribers.pdf:application/pdf}
}

